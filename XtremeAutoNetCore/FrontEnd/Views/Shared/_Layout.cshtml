<!DOCTYPE html>
<html lang="zxx" class="no-js">

<head>
	<!-- Mobile Specific Meta -->
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Favicon-->
	<link rel="shortcut icon" href="img/fav.png">
	<!-- Author Meta -->
	<meta name="author" content="CodePixar">
	<!-- Meta Description -->
	<meta name="description" content="">
	<!-- Meta Keyword -->
	<meta name="keywords" content="">
	<!-- meta character set -->
	<meta charset="UTF-8">
	<!-- Site Title -->
	<title>@ViewData["Title"] - XtremeAuto</title>
	<!-- CSS ============================================= -->
	<link rel="stylesheet" href="~/css/linearicons.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/font-awesome.min.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/themify-icons.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/bootstrap.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/owl.carousel.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/nice-select.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/nouislider.min.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/ion.rangeSlider.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/ion.rangeSlider.skinFlat.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/magnific-popup.css" asp-append-version="true">
	<link rel="stylesheet" href="~/css/main.css" asp-append-version="true">
</head>

<body>

	<!-- Start Header Area -->
	<header class="header_area sticky-header">
		<div class="main_menu">
			<nav class="navbar navbar-expand-lg navbar-light main_box">
				<div class="container">
					<!-- Brand and toggle get grouped for better mobile display -->
					<a class="navbar-brand logo_h" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/img/logo.png" alt="" style="width: 200px;"></a>
					<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<!-- Collect the nav links, forms, and other content for toggling -->
					<div class="collapse navbar-collapse offset" id="navbarSupportedContent">
						<ul class="nav navbar-nav menu_nav ml-auto">
							<li class="nav-item active">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">inicio</a>
							</li>

							<li class="nav-item active">
								<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
							</li>

							<li class="nav-item active">
								<a class="nav-link" asp-area="" asp-controller="CarroModelo" asp-action="Index">Carro Modelo</a>
							</li>

							<li class="nav-item active">
								<a class="nav-link text-dark" asp-area="" asp-controller="Color" asp-action="Index">Color</a>
							</li>
							<li class="nav-item active">
								<a class="nav-link" asp-area="" asp-controller="Ruedum" asp-action="Index">Rueda</a>
							</li>
							<li class="nav-item active">
								<a class="nav-link text-dark" asp-area="" asp-controller="Seguro" asp-action="Index">Seguro</a>
							</li>

							<li class="nav-item  active">
								<a class="nav-link" asp-area="" asp-controller="Tarjetum" asp-action="Index">Tarjeta</a>
							</li>
							<li class="nav-item  active">
								<a class="nav-link text-dark" asp-area="" asp-controller="Rol" asp-action="Index">Rol</a>
							</li>
							<li class="nav-item  active">
								<a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="Index">Usuario</a>
							</li>
							<li class="nav-item  active">
								<a class="nav-link text-dark" asp-area="" asp-controller="CarroVendido" asp-action="Index">CarroVendido</a>
							</li>
							<li class="nav-item  active">
								<a class="nav-link" asp-area="" asp-controller="Ventum" asp-action="Index">Venta</a>
							</li>
							<li class="nav-item  active">
								<a class="nav-link text-dark" asp-area="" asp-controller="Transaccion" asp-action="Index">Transaccion</a>
							</li>

							<li class="nav-item active  active">
								<a class="nav-link" asp-area="" asp-controller="Home" asp-action="Contacto">Contactos</a>

							</li>


						</ul>
						<ul class="nav navbar-nav navbar-right">
							<li class="nav-item"><a href="#" class="cart"><span class="ti-bag"></span></a></li>
							<li class="nav-item">
								<button class="search"><span class="lnr lnr-magnifier" id="search"></span></button>
							</li>
						</ul>
					</div>
				</div>
			</nav>
		</div>
		<div class="search_input" id="search_input_box">
			<div class="container">
				<form class="d-flex justify-content-between">
					<input type="text" class="form-control" id="search_input" placeholder="Buscar aquí">
					<button type="submit" class="btn"></button>
					<span class="lnr lnr-cross" id="close_search" title="Cerrar Búsqueda"></span>
				</form>
			</div>
		</div>
	</header>
	<!-- End Header Area -->
	<!-- start banner Area -->
	<section class="banner-area">
		<div class="container">
			<div class="row fullscreen align-items-center justify-content-start">
				<div class="col-lg-12">

					<!-- single-slide -->
					<div class="row single-slide align-items-center d-flex">
						<div class="col-lg-5 col-md-6">
							<div class="banner-content">
								<h1>XtremeAuto</h1>
								<p>Calidad comproboda</p>

							</div>
						</div>
						<div class="col-lg-7">
							<div class="banner-img">
								<img class="img-fluid" src="~/img/banner/SUVprincipal.png" alt="">
							</div>
						</div>
					</div>

				</div>
			</div>
		</div>
	</section>
	<!-- End banner Area -->


	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<!-- start footer Area -->
	<footer class="footer-area section_gap">
		<div class="container">
			<div class="row">
				<div class="col-lg-3  col-md-6 col-sm-6">
					<div class="single-footer-widget">
						<h6>Acerca de Nosotros</h6>
						<p>
							"Explora nuestra tienda de automóviles, donde encontrarás una variada selección de vehículos de marcas líderes en la industria.
							Nuestro amable equipo de ventas te guiará para encontrar el automóvil ideal que se ajuste a tus preferencias y presupuesto".
						</p>
					</div>
				</div>

				<div class="col-lg-3 col-md-6 col-sm-6">
					<div class="single-footer-widget">
						<h6>Siguenos</h6>
						<p>En  las siguientes redes sociales</p>
						<div class="footer-social d-flex align-items-center">
							<a href="#"><i class="fa fa-facebook"></i></a>
							<a href="#"><i class="fa fa-twitter"></i></a>
							<a href="#"><i class="fa fa-dribbble"></i></a>
							<a href="#"><i class="fa fa-behance"></i></a>
							<a href="#"><i class="fa fa-instagram"></i></a>
						</div>
					</div>
				</div>
			</div>
			<div class="footer-bottom d-flex justify-content-center align-items-center flex-wrap">
				<p class="footer-text m-0">
					<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
					<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
				</p>
			</div>
		</div>
	</footer>
	<!-- End footer Area -->

	<script src="~/js/vendor/jquery-2.2.4.min.js" asp-append-version="true"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
	<script src="~/js/vendor/bootstrap.min.js" asp-append-version="true"></script>
	<script src="~/js/jquery.ajaxchimp.min.js" asp-append-version="true"></script>
	<script src="~/js/jquery.nice-select.min.js" asp-append-version="true"></script>
	<script src="~/js/jquery.sticky.js" asp-append-version="true"></script>
	<script src="~/js/nouislider.min.js" asp-append-version="true"></script>
	<script src="~/js/countdown.js" asp-append-version="true"></script>
	<script src="~/js/jquery.magnific-popup.min.js" asp-append-version="true"></script>
	<script src="~/js/owl.carousel.min.js" asp-append-version="true"></script>
	<!--gmaps Js-->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
	<script src="~/js/gmaps.min.js" asp-append-version="true"></script>
	<script src="~/js/main.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>