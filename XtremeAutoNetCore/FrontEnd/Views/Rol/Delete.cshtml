@model FrontEnd.Models.RolViewModel

@{
    ViewData["Title"] = "Delete";
}


<h1>Eliminar Rol</h1>

<h3>¿Estás seguro de que quieres eliminar este Rol?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-md-12 form-groupm-2">
            @Html.DisplayNameFor(model => model.RolId)
        </dt>
        <dd class="col-md-12 form-groupm-10">
            @Html.DisplayFor(model => model.RolId)
        </dd>
        <dt class="col-md-12 form-groupm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-md-12 form-groupm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>

    <form asp-action="Delete">
        @Html.HiddenFor(Model=>Model.RolId)
        <input type="submit" value="Eliminar" class="genric-btn danger circle" /> |
        <a asp-action="Index" class="genric-btn primary small">Regresar</a>
    </form>
</div>